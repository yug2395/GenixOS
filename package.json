{
  "name": "genix-os",
  "version": "1.0.0",
  "description": "GENIX OS - The OS for Students, by Students",
  "main": "dist/main/main.js",
  "scripts": {
    "build": "npm run build:main && npm run build:renderer && npm run build:backend",
    "build:main": "tsc -p tsconfig.main.json",
    "build:renderer": "webpack --config webpack.renderer.config.js --mode production",
    "build:backend": "tsc -p tsconfig.backend.json",
    "build:c": "cd c-engine && make",
    "dev": "concurrently \"npm run dev:main\" \"npm run dev:renderer\" \"npm run dev:backend\"",
    "dev:main": "tsc -p tsconfig.main.json --watch",
    "dev:renderer": "webpack serve --config webpack.renderer.config.js --mode development",
    "dev:backend": "tsc -p tsconfig.backend.json && node dist/backend/server.js",
    "start:backend": "node dist/backend/server.js",
    "start": "electron .",
    "start:dev": "electron . --dev",
    "lint": "eslint . --ext .ts,.tsx",
    "format": "prettier --write \"**/*.{ts,tsx,json,css}\""
  },
  "keywords": [
    "genix",
    "os",
    "electron",
    "react",
    "desktop"
  ],
  "author": "",
  "license": "MIT",
  "devDependencies": {
    "@types/node": "^20.10.0",
    "@types/react": "^18.2.45",
    "@types/react-dom": "^18.2.18",
    "@typescript-eslint/eslint-plugin": "^6.15.0",
    "@typescript-eslint/parser": "^6.15.0",
    "autoprefixer": "^10.4.16",
    "concurrently": "^8.2.2",
    "css-loader": "^6.8.1",
    "electron": "^28.0.0",
    "eslint": "^8.56.0",
    "eslint-plugin-react": "^7.33.2",
    "eslint-plugin-react-hooks": "^4.6.0",
    "file-loader": "^6.2.0",
    "html-webpack-plugin": "^5.6.0",
    "postcss": "^8.4.32",
    "postcss-loader": "^7.3.4",
    "prettier": "^3.1.1",
    "style-loader": "^3.3.3",
    "tailwindcss": "^3.4.0",
    "ts-loader": "^9.5.1",
    "typescript": "^5.3.3",
    "webpack": "^5.89.0",
    "webpack-cli": "^5.1.4",
    "webpack-dev-server": "^4.15.1"
  },
  "dependencies": {
    "dotenv": "^17.2.3",
    "monaco-editor": "^0.45.0",
    "node-fetch": "^3.3.2",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "ws": "^8.16.0",
    "xterm": "^5.3.0",
    "xterm-addon-fit": "^0.8.0"
  }
}
